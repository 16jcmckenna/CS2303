#ifndef EVENT_H_
#define EVENT_H_

namespace std {

class Event {
public:
	int num_customers;
	int num_tellers;
	int sim_time;
	float avg_serv_time;

	Event();
	virtual ~Event();
};

} /* namespace std */

#endif /* EVENT_H_ */
