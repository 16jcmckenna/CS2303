#include <stdio.h>

int inputYear;


int getStartingDay (d, m, y) {

	if(m == 1){
		m = 13;
		y--;
	}
	else if(m == 2){
		m = 14;
		y--;
	}

	return (d + 2*m + 3*(m+1)/5 + y + y/4 - y/100 + y/400 + 2) % 7;

}

void printCalender(year, day) {
	printf("%i -- %i", year, day);

	/*for(int m = 3; m <= 14; m++) {
		printMonth(year, m, );
	}*/
}


int main (void) {
	printf("Enter a year: ");
	scanf("%i", &inputYear);

	printCalender(inputYear, getStartingDay(1,1,inputYear));

	return 0;
}


